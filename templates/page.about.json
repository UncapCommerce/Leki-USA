    "4b2c5915-931a-4ed0-a68a-009e9ded6efc": {
      "type": "section-text",
      "blocks": {
        "template--18722030354726__4b2c5915-931a-4ed0-a68a-009e9ded6efc-1683698935b6d74e0f-1": {
          "type": "heading",
          "settings": {
            "text": "<p>TOP PERFORMANCE<\/p>",
            "size": 100,
            "heading_highlight_type": "italics"
          }
        },
        "template--18722030354726__4b2c5915-931a-4ed0-a68a-009e9ded6efc-1683698935b6d74e0f-2": {
          "type": "text",
          "settings": {
            "text": "<p>LEKI stands for ultimate performance. We proudly support athletes in delivering their top performance and in celebrating their successes. The connection to elite sports is the foundation of our product development work.<\/p>",
            "size": 100
          }
        }
      },
      "block_order": [
        "template--18722030354726__4b2c5915-931a-4ed0-a68a-009e9ded6efc-1683698935b6d74e0f-1",
