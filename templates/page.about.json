    "4f702ff1-69b3-4a17-955c-96230c066a2e": {
      "type": "about-image-with-text",
      "blocks": {
        "79704ca8-2a95-44cb-a946-7eb360d4591f": {
          "type": "image-with-text",
          "settings": {
            "image": "shopify:\/\/shop_images\/SUPPORT_FOR_ATHLETES.jpg",
            "subheading": "SUPPORT FOR ATHLETES",
            "subheading_clr": "#bf0038",
            "title": "",
            "text_content": "Partnership also takes first place when it comes to our athletes. LEKI draws on a unique pool of expertise, with over 1,000 athletes involved in alpine skiing, Nordic sports, trail running and mountaineering. This partnership is not just about providing each athlete with the best equipment. It's about standing by athletes as a true partner and being there for them in every way. This requires skill, commitment and patience. We do all this for our athletes, to give them an extra boost and to enhance their performance.",
            "text_alignment": "center",
            "text_color": "text-dark"
          }
        }
      },
      "block_order": [
        "79704ca8-2a95-44cb-a946-7eb360d4591f"
      ],
