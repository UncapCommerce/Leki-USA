    "99e70c64-645d-4f82-8b8d-dfa3043e7c2c": {
      "type": "section-text",
      "blocks": {
        "template--18722030354726__99e70c64-645d-4f82-8b8d-dfa3043e7c2c-1683698935b6d74e0f-1": {
          "type": "heading",
          "settings": {
            "text": "<p>QUALITY<\/p>",
            "size": 100,
            "heading_highlight_type": "italics"
          }
        },
        "template--18722030354726__99e70c64-645d-4f82-8b8d-dfa3043e7c2c-1683698935b6d74e0f-2": {
          "type": "text",
          "settings": {
            "text": "<p>We strive for the highest quality in the design and manufacture of our products. High-quality, reliable and durable products ensure an optimal user experience and the satisfaction of our customers.<\/p>",
            "size": 100
          }
        }
      },
      "block_order": [
        "template--18722030354726__99e70c64-645d-4f82-8b8d-dfa3043e7c2c-1683698935b6d74e0f-1",
